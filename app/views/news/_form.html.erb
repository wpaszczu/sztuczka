<%= simple_form_for(@news) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :co %>
    <%= f.input :czego %>
    <%= f.input :czas1 %>
    <%= f.input :czas2 %>
    <%= f.input :czas3 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
