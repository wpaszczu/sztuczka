<%= simple_form_for @news, :html => { :class => 'form-horizontal' } do |f| %>

  <%= f.input :foto_news, as: :file %>
  <%= f.input :co %>
  <%= f.input :czego %>
  <%= f.input :czas1 %>
  <%= f.input :czas2 %>
  <%= f.input :czas3 %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                news_index_path, :class => 'btn btn-default' %>
<% end %>
